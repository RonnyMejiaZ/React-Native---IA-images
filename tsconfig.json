    "app/(tabs)/_layout.tsx",
    "app/[gameslug].tsx",
    "app/[gameslug].tsx",
    "components/Logo.tsx",
    "app/[imageslug].jsx", "components/Main.tsx", "components/PromptCard.tsx", "components/PromptCard.tsx"  ],
  "extends": "expo/tsconfig.base"
}
